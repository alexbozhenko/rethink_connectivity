bench:
	@echo "\n===============================go test -bench=. -benchtime=1s --cpu 1"
	@go test -bench=. -benchtime=1s --cpu 1
	@echo "\n===============================go test -bench=. -benchtime=1s --cpu 5"
	@go test -bench=. -benchtime=1s --cpu 5
	@echo "\n===============================go test -bench=. -benchtime=1s --cpu 25"
	@go test -bench=. -benchtime=1s --cpu 25
	@echo "\n===============================go test -bench=. -benchtime=1s --cpu 50"
	go test -bench=. -benchtime=1s --cpu 50
	@echo "\n===============================go test -bench=. -benchtime=1s --cpu 100"
	@go test -bench=. -benchtime=1s --cpu 100
